{
  "name": "roots-discourse-release-action",
  "version": "1.0.0",
  "description": "GitHub Action to create a Discourse post from a GitHub release",
  "main": "index.js",
  "scripts": {
    "build": "ncc build",
    "prepare": "ncc build -o dist --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roots/.github.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "Roots",
    "Discourse"
  ],
  "author": "Scott Walkinshaw <scott@roots.io>",
  "license": "MIT",
  "homepage": "https://github.com/roots",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/http-client": "^2.1.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.6.2"
  }
}
